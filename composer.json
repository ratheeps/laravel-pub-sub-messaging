{
    "name": "ratheeps/laravel-pub-sub-messaging",
    "description": "A simple Laravel service provider which adds a new queue connector to handle SNS subscription queues.",
    "license": "MIT",
    "keywords": ["laravel", "lumen", "queue", "sns subscription", "sns publishing", "microservices", "event-driven", "aws", "sqs", "sns"],
    "type": "library",
    "authors": [
        {
            "name": "Ratheep",
            "email": "ratheepan93@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.0|>=8.0",
        "aws/aws-sdk-php": "^3.189.0",
        "league/flysystem": "^1.1",
        "illuminate/container": "^v7.0.0|^v8.0.0",
        "illuminate/contracts": "^v7.0.0|^v8.0.0",
        "illuminate/filesystem": "^v7.0.0|^v8.0.0",
        "illuminate/queue": "^v7.0.0|^v8.0.0",
        "illuminate/support": "^v7.0.0|^v8.0.0",
        "ext-json": "*"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Ratheeps\\PubSubMessaging\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ratheeps\\PubSubMessaging\\PubSubMessagingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
